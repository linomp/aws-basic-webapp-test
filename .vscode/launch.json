{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "HelloWorldFunction:index.handler (nodejs14.x)",
            "invokeTarget": {
                "target": "code",
                "projectRoot": "${workspaceFolder}/HelloWorldFunction",
                "lambdaHandler": "index.handler"
            },
            "lambda": {
                "runtime": "nodejs14.x",
                "payload": {},
                "environmentVariables": {}
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "lambda-nodejs14.x:HelloWorldFunction (nodejs14.x)",
            "invokeTarget": {
                "target": "template",
                "templatePath": "${workspaceFolder}/lambda-nodejs14.x/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "lambda": {
                "payload": {},
                "environmentVariables": {},
                "runtime": "nodejs14.x"
            }
        },
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "name": "API lambda-nodejs14.x:HelloWorldFunction (nodejs14.x)",
            "invokeTarget": {
                "target": "api",
                "templatePath": "${workspaceFolder}/lambda-nodejs14.x/template.yaml",
                "logicalId": "HelloWorldFunction"
            },
            "api": {
                "path": "/hello",
                "httpMethod": "get",
                "payload": {
                    "json": {
                        "firstName": "Ada",
                        "lastName": "Lovelace"
                    }
                }
            },
            "lambda": {
                "runtime": "nodejs14.x"
            }
        }
    ]
}